<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<wsdl:definitions xmlns:tns="SituationHandler/SituationHandler/"
	xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap/" xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/"
	xmlns:xsd="http://www.w3.org/2001/XMLSchema" name="SituationHandler"
	targetNamespace="SituationHandler/SituationHandler/">

	<wsdl:portType name="Request"></wsdl:portType>
	<wsdl:portType name="Response"></wsdl:portType>
	<wsdl:binding name="RequestServiceBinding" type="tns:Request">
		<soap:binding style="document"
			transport="http://schemas.xmlsoap.org/soap/http" />
	</wsdl:binding>
	<wsdl:binding name="ResponseServiceBinding" type="tns:Response">
		<soap:binding style="document"
			transport="http://schemas.xmlsoap.org/soap/http" />
	</wsdl:binding>
	<wsdl:service name="RequestService">
		<wsdl:port name="RequestServicePort" binding="tns:RequestServiceBinding">
			<soap:address location="http://localhost:8081/RequestEndpoint" />
		</wsdl:port>
	</wsdl:service>
	<wsdl:service name="ResponseService">
		<wsdl:port name="ResponseServicePort" binding="tns:ResponseServiceBinding">
			<soap:address location="http://localhost:8081/AnswerEndpoint" />
		</wsdl:port>
	</wsdl:service>
</wsdl:definitions>

	
